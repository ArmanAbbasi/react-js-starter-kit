{
  "name": "react-js-starter-kit",
  "author": "Arman Abbasi",
  "license": "GPL-3.0",
  "description": "A starter kit for reactJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArmanAbbasi/react-js-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/ArmanAbbasi/react-js-starter-kit/issues"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config ./src/config/webpack.config.babel.js --progress --hide-modules",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --config ./src/config/webpack.config.babel.js --progress --hide-modules",
    "dev": "npm run clean && npm run build:dev && cross-env NODE_ENV=development node src/init",
    "prod": "npm run clean && npm run build:prod && cross-env NODE_ENV=production node src/init"
    "serve": "cross-env NODE_ENV=production node src/init"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.2",
    "postcss-smart-import": "^0.6.9",
    "precss": "^1.4.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "express-handlebars": "^3.0.0",
    "helmet": "^3.4.1",
    "history": "^4.5.1",
    "morgan": "^1.8.1",
    "node-jsx": "^0.13.3",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-logger": "^2.8.1",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "static-asset": "^0.6.0"
  }
}
